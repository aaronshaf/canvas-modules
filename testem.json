{
  "framework": "qunit",
  "launch_in_ci": [
    "Firefox",
    "PhantomJS"
  ],
  "launch_in_dev": [
    "Chrome"
  ],
  "src_files": [
    "components/*.coffee",
    "config/*.coffee",
    "controllers/*.coffee",
    "lib/*",
    "models/*.coffee",
    "routes/*.coffee",
    "templates/*.coffee",
    "views/*.coffee"
  ],
  "serve_files": [
    "vendor/requirejs/require.js",
    "vendor/sinon/lib/sinon.js",
    "vendor/sinon/lib/sinon/assert.js",
    "vendor/sinon/lib/sinon/util/fake_server.js",
    "vendor/sinon-qunit/lib/sinon-qunit.js",
    "compiled/dev/main.js"
  ]
}
